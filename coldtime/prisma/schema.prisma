// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./coldtime.db"
}

model Device {
  id   String @id @default(cuid())
  ip   String
  port Int    @default(56000)

  name String
}
